package tools;

import java.awt.event.KeyEvent;

import javax.swing.ImageIcon;

/**
 * A Rectangle Tool.
 * 
 * @author Shannon Murphy
 * @version 20 February 2015
 *
 */
public class RectangleTool extends AbstractTool {

    /**
     * Constructs a new Rectangle Tool with the specified name, Icon, and mnemonic.
     */
    public RectangleTool() {
        super("Rectangle", new ImageIcon("./images/rectangle.gif"), KeyEvent.VK_R);
    }
}